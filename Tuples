tup1=('1','2','3','4','5')
print(tuple)
tup=('one', 'Two', 'Three')
print(tup1+tup)
list1=['Hema','Harshitha','Rani']
print(tuple(list1))
print(tuple(list1)+tup1)
tup2=tuple('Kothamasu')
print(tup2)
print(list(tup))
tup3=(tup1,tup,tup2)
print(tup3)
print(tup[1])
print(tup[1][1])
print(tup1[:3])
print(tup1[1:4])
print(tup[:])
print(tup1[-5:-1])
del tup
print(tup)
